# API Configuration
# Base URL for API endpoints (defaults to /api)
NEXT_PUBLIC_API_BASE_URL=/api

# Node Environment
NODE_ENV=development

# Next.js Configuration
# Disable Next.js telemetry (optional)
NEXT_TELEMETRY_DISABLED=1

# Sentry Configuration
# Get your DSN from https://sentry.io/settings/{org}/projects/{project}/keys/
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_DSN=
SENTRY_ORG=
SENTRY_PROJECT=

# Enable Sentry in development (optional, defaults to production only)
# Set to "true" to enable Sentry in development for testing
SENTRY_ENABLED=
